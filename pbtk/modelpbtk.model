};

BW = 70;
Clmetabolismc = 0.203;
hematocrit = 0.44;
kgutabs = 1;
Kkidney2pu = 0;
Kliver2pu = 0;
Krest2pu = 0;
Kgut2pu = 0;
Klung2pu = 0;
Qcardiacc = 4.8;
Qgfrc = 0.108;
Qgutf = 0.205;
Qkidneyf = 0.221;
Qliverf = 0.0536;
Vartc = 0.0487;
Vgutc = 0.0158;
Vkidneyc = 0.00119;
Vliverc = 0.02448;
Vlungc = 0.00723;
Vrestc = 0.77654;
Vvenc = 0.0487;
Fraction_unbound_plasma = 0.0682;
Rblood2plasma = 0.0;
Clmetabolism = 0.0;
